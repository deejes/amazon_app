<h1>Hello Admin</h1>


<p>Product Count: <%= Product.count%></p>
<p>User Count: <%= User.count%></p>
<p>Review Count: <%= Review.count%></p>


<hr>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
  </thead>
      <tbody>
        <%@users.each do |user|%>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
        </tr>
        <%end%>
      </tbody>
</table>
